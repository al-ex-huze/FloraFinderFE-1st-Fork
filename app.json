{
  "expo": {
    "name": "FloraFinder",
    "slug": "FloraFinder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/flowericons/flowerIcon7.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/FloraFinderLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "white"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/FloraFinderLogo.png",
        "backgroundColor": "white"
      }
    },
    "web": {
      "favicon": "./assets/flowerIcon7.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
